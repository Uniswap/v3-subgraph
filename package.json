{
  "name": "uniswap-v3-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/graphprotocol/uniswap-v3-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "lint": "eslint . --ext .ts --fix",
    "build": "run-s codegen && graph build",
    "codegen": "graph codegen --output-dir src/types/",
    "deploy": "graph deploy uniswap-v3 --node https://api.haven1.0xgraph.xyz/deploy --ipfs https://api.haven1.0xgraph.xyz/ipfs --deploy-key $DEPLOY_KEY",
    "devnet:build": "graph build --network haven1-devnet",
    "testnet:build": "graph build --network haven1-testnet",
    "mainnet:build": "graph build --network haven1-mainnet",
    "local:graph": "NETWORK='localhost:http://host.docker.internal:8545' docker-compose up",
    "local:graph:devnet": "NETWORK='haven1-devnet:https://rpc.staging.haven1.org' docker-compose up",
    "local:graph:testnet": "NETWORK='haven1-testnet:https://testnet-rpc1.haven1.org' docker-compose up",
    "local:build": "yarn build --network localhost",
    "local:create": "graph create --node http://localhost:8020 uniswap-v3-local",
    "local:deploy": "graph deploy -l 0.0.1 --node http://localhost:8020 --ipfs http://localhost:5001 uniswap-v3",
    "local:graph:clean": "docker-compose down -v && docker-compose rm -v && rm -rf data/ipfs data/postgres",
    "local:remove": "graph remove --node http://localhost:8020 uniswap-v3",
    "local:setup": "yarn codegen && yarn build && yarn local:create && yarn local:deploy",
    "local:watch": "graph deploy uniswap-v3-local --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001",
    "build:docker": "docker build -t matchstick .",
    "buildonly": "graph build",
    "test": "graph test -d",
    "test:no-tty": "docker run -i --rm --mount type=bind,source=$INIT_CWD,target=/matchstick matchstick"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.95.0",
    "@graphprotocol/graph-ts": "^0.37.0",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "@uniswap/eslint-config": "^1.2.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^9.1.0",
    "matchstick-as": "^0.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.0",
    "typescript": "^5.7.3"
  }
}
